{
  "name": "its-me",
  "version": "0.1.0",
  "description": "Personal API for Enzo Volkmann",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "start": "nodemon",
    "start:dev": "ts-node -r tsconfig-paths/register src/main.ts",
    "build": "rimraf dist && tsc",
    "start:prod": "node dist/main.js",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./jest-e2e.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/evolkmann/its-me"
  },
  "author": {
    "name": "Enzo Volkmann",
    "email": "enzo@enzo-volkmann.de"
  },
  "dependencies": {
    "@nestjs/common": "4.6.6",
    "@nestjs/core": "4.6.6",
    "@nestjs/swagger": "1.2.2",
    "@nestjs/testing": "4.6.6",
    "nodemailer": "4.6.4",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.5.10"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.8",
    "@types/express": "4.11.1",
    "@types/jest": "22.2.3",
    "@types/node": "9.6.6",
    "@types/nodemailer": "4.6.0",
    "@types/supertest": "2.0.4",
    "jest": "22.4.3",
    "nodemon": "1.17.3",
    "prettier": "1.12.1",
    "supertest": "3.0.0",
    "ts-jest": "22.4.4",
    "ts-node": "6.0.0",
    "tsconfig-paths": "3.3.1",
    "tslint": "5.9.1",
    "typescript": "2.6.2"
  },
  "license": "MIT"
}
